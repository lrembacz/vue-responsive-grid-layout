    "build-wc": "vue-cli-service build --target wc --name vue-responsive-grid-layout ./src/components/*.vue",
    "build-wc-async": "vue-cli-service build --target wc-async --name vue-responsive-grid-layout ./src/components/*.vue",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.6.7",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "^7.3.0"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js",
    "types/*"
