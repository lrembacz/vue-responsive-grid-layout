{
    "compilerOptions": {
        "moduleResolution": "node",
        "module": "esnext",
        "target": "esnext",
        "allowJs": true,
        "noImplicitAny": true,
        "noImplicitThis": false,
        "preserveConstEnums": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "sourceMap": true,
        "lib": [
            "dom",
            "esnext"
        ],
        "strict": true,
        "declaration": true,
        "types": [
            "@types/node",
            "@types/jest",
            "@types/lodash.isequal"
        ]
    },
    "files": [
        "node_modules/@types/node/globals.d.ts",
        "types/shims-vue.d.ts"
    ],
    "exclude": [
        "dist",
        "node_modules",
        "developPreview"
    ]
}
